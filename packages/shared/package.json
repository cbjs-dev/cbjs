{
  "name": "@cbjsdev/shared",
  "version": "1.8.0-alpha.0",
  "description": "Shared utils and types for Cbjs",
  "author": "Jonathan MASSUCHETTI <jonathan.massuchetti@dappit.fr>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
