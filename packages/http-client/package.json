{
  "name": "@cbjsdev/http-client",
  "version": "1.7.0",
  "description": "A type safe HTTP client for Couchbase.",
  "keywords": [
    "couchbase",
    "client",
    "http",
    "typescript"
  ],
  "author": "Jonathan MASSUCHETTI <jonathan.massuchetti@dappit.fr>",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/src/index.js",
