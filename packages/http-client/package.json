{
  "name": "@cbjs/http-client",
  "version": "0.1.0",
  "description": "A type safe HTTP client for Couchbase.",
  "keywords": ["couchbase", "client", "http", "typescript"],
  "author": "Jonathan MASSUCHETTI <jonathan.massuchetti@dappit.fr>",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/http-client.cjs",
        "types": "./dist/http-client.d.cts"
      },
      "import": {
        "default": "./dist/http-client.js",
        "types": "./dist/http-client.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/http-client.cjs",
  "module": "dist/http-client.js",
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --passWithNoTests"
  },
  "dependencies": {
    "promise-retry": "^2.0.1",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/promise-retry": "^1.1.6"
  }
}
