{
  "name": "@cbjs/http-client",
  "version": "0.1.0",
  "description": "A type safe HTTP client for Couchbase.",
  "keywords": [
    "couchbase",
    "client",
    "http",
    "typescript"
  ],
  "author": "Jonathan MASSUCHETTI <jonathan.massuchetti@dappit.fr>",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "cp package.json dist/package.json && tsc -p tsconfig.build.json",
    "build:watch": "cp package.json dist/package.json && tsc --watch -p tsconfig.build.json",
    "test": "vitest --run --passWithNoTests"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "NOTICE"
  ],
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "ts-retry-promise": "^0.8.0"
  },
  "devDependencies": {
    "@types/promise-retry": "^1.1.6"
  }
}
