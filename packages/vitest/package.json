{
  "name": "@cbjs/vitest",
  "version": "1.0.0",
  "description": "Vitest extended tests and fixtures for your tests with Couchbase",
  "keywords": [
    "couchbase",
    "vitest",
    "fixture",
    "test"
  ],
  "author": "Jonathan MASSUCHETTI <jonathan.massuchetti@dappit.fr>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbjs-dev/cbjs.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "bugs": {
    "url": "https://github.com/cbjs-dev/cbjs/issues"
  },
  "scripts": {
    "prebuild": "[ -d dist/. ] || mkdir dist && cp package.json dist/package.json",
    "build": "tsc -p tsconfig.build.json",
    "dev": "npm run build -- --watch",
    "test": "vitest --run"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE",
    "NOTICE"
  ],
  "dependencies": {
    "@cbjs/cbjs": "*",
    "@cbjs/http-client": "*",
    "vitest": "^1.2.0"
  },
  "devDependencies": {
    "http-client": "^4.3.1",
    "zod": "^3.22.4"
  }
}
