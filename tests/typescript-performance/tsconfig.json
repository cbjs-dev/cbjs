{
  "compilerOptions": {
    "lib": ["es2022"],
    "target": "es2022",
    "strict": true,
    "noEmit": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "moduleResolution": "node16",
    "module": "node16",
    "declaration": true,
    "sourceMap": true,
    "resolveJsonModule": true
  }
}
